
<div class="card p-4 full-height">
    <div class="card-header">
        <h3 class="d-inline-block">{{ 'properties'|trans|capitalize }}</h3>
    </div>
    <div class="card-body">
        {% if properties is defined %}
            <table class="table table-hover">
                <thead>
                <tr>
                    <th scope="col">{{ 'property'|trans|capitalize }}</th>
                    <th scope="col">{{ 'value'|trans|capitalize }}</th>
                </tr>
                </thead>
                <tbody>
                {% for key,value in properties %}
                    <tr>
                        <td>{{ key }}</td>
                        <td>{{ dump(value) }}</td>
                    </tr>
                {% endfor %}
                </tbody>
            </table>
        {% endif %}
    </div> <!-- /.card-body -->

    <div class="card-action">
    </div><!-- /.card-action -->
</div><!-- /.card -->
