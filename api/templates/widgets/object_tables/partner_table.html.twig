<div class="col-md-12">
    <div class="card full-height">
        <table class="table table-hover">
            <thead>
            <tr>
                <th scope="col">BSN</th>
                <th scope="col">Naam</th>
                <th scope="col">Geslacht</th>
                <th scope="col">Geboorte</th>
                <th scope="col"></th>
            </tr>
            </thead>
            <tbody>
			{% for index, partner in partners._embedded.item %}
            <tr>
                <td>{{ partner.burgerservicenummer }}</td>
                <td>{{ partner.naam }}</td>
                <td>{{ partner.geslachtsaanduiding }}</td>
                <td>{{ partner.geboorte }}</td>
                <td><a href="{{ path('app_brp_edit',{'id':partner.id}) }}">Edit</a></td>
            </tr>
			{% endfor %}
            </tbody>
        </table>
    </div>
</div>
