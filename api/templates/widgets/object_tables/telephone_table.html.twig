<div class="col-md-12">
    <div class="card full-height">
        <table class="table table-hover">
            <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Name</th>
                <th scope="col">Telephone</th>
                <th scope="col"></th>
            </tr>
            </thead>
            <tbody>
            {% for index, telephone in telephones %}
                <tr>
                    <td>{{ telephone.id }}</td>
                    <td>{{ telephone.name }}</td>
                    <td>{{ telephone.telephone }}</td>
                    <td><a href="{{ path('app_contact_edit',{'id':telephone.id}) }}">Edit</a></td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
</div>
