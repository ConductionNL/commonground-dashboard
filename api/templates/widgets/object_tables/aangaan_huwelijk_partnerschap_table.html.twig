<div class="col-md-12">
    <div class="card full-height">
        <table class="table table-hover">
            <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Datum</th>
                <th scope="col">In onderzoek</th>
                <th scope="col">Quantity</th>
                <th scope="col">Price</th>
                <th scope="col"></th>
            </tr>
            </thead>
            <tbody>
			{% for index, aangaan_huwelijk_partnerschap in aangaan_huwelijk_partnerschappen._embedded.item %}
            <tr>
                <td>{{ aangaan_huwelijk_partnerschap.uuid }}</td>
                <td>{{ aangaan_huwelijk_partnerschap.offer }}</td>
                <td>{{ aangaan_huwelijk_partnerschap.product }}</td>
                <td>{{ aangaan_huwelijk_partnerschap.quantity }}</td>
                <td>{{ aangaan_huwelijk_partnerschap.price }}</td>
                <td><a href="{{ path('app_brp_edit',{'id':aangaan_huwelijk_partnerschap.id}) }}">Edit</a></td>
            </tr>
			{% endfor %}
            </tbody>
        </table>
    </div>
</div>
