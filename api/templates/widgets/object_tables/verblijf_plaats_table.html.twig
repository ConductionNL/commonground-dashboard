<div class="col-md-12">
    <div class="card full-height">
        <table class="table table-hover">
            <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Postcode</th>
                <th scope="col">Straat</th>
                <th scope="col">Huisnummer</th>
                <th scope="col">Huistoevoeging</th>
                <th scope="col">Huisletter</th>
                <th scope="col">Woonplaats</th>
                <th scope="col">Land</th>
                <th scope="col"></th>
            </tr>
            </thead>
            <tbody>
			{% for index, verblijf_plaats in verblijf_plaatsen._embedded.item %}
            <tr>
                <td>{{ verblijf_plaats.uuid }}</td>
                <td>{{ verblijf_plaats.postcode }}</td>
                <td>{{ verblijf_plaats.straatnaam }}</td>
                <td>{{ verblijf_plaats.huisnummer }}</td>
                <td>{{ verblijf_plaats.huisnummertoevoeging }}</td>
                <td>{{ verblijf_plaats.huisletter }}</td>
                <td>{{ verblijf_plaats.woonplaatsnaam }}</td>
                <td>{{ verblijf_plaats.landVanwaarIngeschreven }}</td>
                <td><a href="{{ path('app_brp_edit',{'id':verblijf_plaats.id}) }}">Edit</a></td>
            </tr>
			{% endfor %}
            </tbody>
        </table>
    </div>
</div>
