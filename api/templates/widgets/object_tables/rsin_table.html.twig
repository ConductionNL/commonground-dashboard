<div class="col-md-12">
    <div class="card full-height">
        <table class="table table-hover">
            <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">RSIN</th>
                <th scope="col">KVK</th>
                <th scope="col">Gemeente code</th>
                <th scope="col"></th>
            </tr>
            </thead>
            <tbody>
            {% for index, rsinNumber in rsinNumbers %}
                <tr>
                    <td>{{ rsinNumber.id }}</td>
                    <td>{{ rsinNumber.rsin }}</td>
                    <td>{{ rsinNumber.kvk }}</td>
                    <td>{{ rsinNumber.gemeente_code }}</td>
                    <td><a href="{{ path('app_landelijk_edit',{'id':rsinNumber.id}) }}">Edit</a></td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
</div>
