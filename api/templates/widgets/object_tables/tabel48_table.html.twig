<div class="col-md-12">
    <div class="card full-height">
        <table class="table table-hover">
            <thead>
            <tr>
                <th scope="col">ID</th>
                <th scope="col">Nederlandse reisdocument</th>
                <th scope="col">Omschrijving</th>
                <th scope="col">Datum ingang</th>
                <th scope="col">Datum einde</th>
                <th scope="col"></th>
            </tr>
            </thead>
            <tbody>
            {% for index, tabel48 in tabel48s %}
                <tr>
                    <td>{{ tabel48.id }}</td>
                    <td>{{ tabel48.nederlandse_reisdocument }}</td>
                    <td>{{ tabel48.omschrijving }}</td>
                    <td>{{ tabel48.datum_ingang }}</td>
                    <td>{{ tabel48.datum_einde }}</td>
                    <td><a href="{{ path('app_landelijk_edit',{'id':tabel48.id}) }}">Edit</a></td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
</div>
