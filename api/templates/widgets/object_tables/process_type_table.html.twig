<div class="col-md-12">
    <div class="card full-height">
        <table class="table table-hover">
            <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Description</th>
                <th scope="col">Source organization</th>
                <th scope="col">Request</th>
                <th scope="col"></th>
            </tr>
            </thead>
            <tbody>
            {% for index, processType in processTypes %}
                <tr>
                    <td>{{ processType.name }}</td>
                    <td>{{ processType.description }}</td>
                    <td>{{ processType.source_organization }}</td>
                    <td>{{ processType.request }}</td>
                    <td><a href="{{ path('app_process_edit',{'id':processType.id}) }}">Edit</a></td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
</div>
