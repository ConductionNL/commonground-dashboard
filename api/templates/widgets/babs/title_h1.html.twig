{#                        <a href="{{ path('app_vrc_requests',{'status': "cancelled"}) }}" class=" mx-1 btn btn-secondary btn-round">{{ 'cancelled'|trans|capitalize }}</a>#}



{#                        {% endif %}#}
                        {#                <a href="#" class="btn btn-white btn-border btn-round mr-2">Manage</a>#}
                        {% if view is defined %}<a href="{{ path(view, {id: 'new'}) }}"
                                                   class=" mx-1 btn btn-secondary btn-round">{{ 'new'|trans|capitalize }} {{ 'add'|trans }} </a> {% endif %}
                    </div>
                {% endblock %}
            </div>
        </div>
    </div>
</div>

